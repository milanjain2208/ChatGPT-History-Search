{
    "manifest_version": 3,
    "name": "ChatGPT History Search",
    "version": "1.0",
    "description": "Search through ChatGPT history.",
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "https://chat.openai.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://chatgpt.com/*"],
        "js": ["content.js"]
      }
    ]
  }